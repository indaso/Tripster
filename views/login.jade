extends layout

block content
	h1 Log on to your Tripster account
	a(href="/") Home			
	a(href="/login") Login
	a(href="/signup") Sign Up
	a(href='/myprofile') MyProfile

	form(method='post')
		p
			label(for='username') Username
			input(type='text', name='username')
		p
			label(for='password') Password
			input(type='password', name='password')
		input(type='submit')
  
