extends layout

block content
	h1 Log on to your Tripster account
	a(href="/") Home	
	br		
	a(href="/login") Login
	br
	a(href="/signup") Sign Up

	form(method='post')
		p
			label(for='username') Username
			input(type='text', name='username')
		p
			label(for='password') Password
			input(type='password', name='password')
		input(type='submit')
  
