extends layout

block content
	h1 Log on to your Tripster account
	a(href="/") Home			
	a(href="/login") Login
	a(href="/signup") Sign Up

	form(method='post')
		p
			label(for='email') Email
			input(type='text', name='email')
		p
			label(for='password') Password
			input(type='password', name='password')
		input(type='submit')
  
